#include<bits/stdc++.h>
using namespace std;
void frequencyOfElement(int arr[], int n)
{
    unordered_map<int, int> m;
    for(int i=0;i<n;i++)
    {
        m[arr[i]]++;
    }
    for(auto x:m)
    {
        cout<<x.first<<" "<<x.second;
        cout<<endl;
    }
}
int main()
{   
    int arr[]={1,2,2,3,4,4,6,6};
    frequencyOfElement(arr,8);
    return 0;